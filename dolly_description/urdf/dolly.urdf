<?xml version="1.0" ?>
<robot name="Dolly"
  xmlns:xacro="http://ros.org/wiki/xacro">
    <link name='chassis'>
      <inertial>
        <mass value="1.0" />
        <inertia ixx="0.0227" ixy="0" ixz="0" iyy="0.0852" iyz="0" izz="0.1041" />
      </inertial>
      <visual name='visual'>
        <geometry>
          <box size="1.0 0.5 0.15" />
        </geometry>
      </visual>
      <visual name="box">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="1.0 0.5 0.15" />
        </geometry>
      </visual>
    </link>
    <link name='caster'>
      <inertial>
        <mass value="0.0415553" />
        <inertia ixx="7.97741e-05" ixy="0" ixz="0" iyy="7.97741e-05" iyz="0" izz="7.97741e-05" />
      </inertial>
      <visual name='visual'>
        <origin xyz="0 0.0 0" rpy="0 0 0"/>
        <geometry>
          <sphere radius="0.1" />
        </geometry>
      </visual>
      <collision name='collision'>
        <origin xyz="0 0.0 0" rpy="0 0 0"/>
        <geometry>
          <sphere radius="0.1" />
        </geometry>
      </collision>
    </link>
     <link name='caster_front'>
      <inertial>
        <mass value="0.0415553" />
        <inertia ixx="7.97741e-05" ixy="0" ixz="0" iyy="7.97741e-05" iyz="0" izz="7.97741e-05" />
      </inertial>
      <visual name='visual'>
        <origin xyz="0 0.0 0" rpy="0 0 0"/>
        <geometry>
          <sphere radius="0.1" />
        </geometry>
      </visual>
      <collision name='collision'>
        <origin xyz="0 0.0 0" rpy="0 0 0"/>
        <geometry>
          <sphere radius="0.1" />
        </geometry>
      </collision>
    </link>

    <link name='left_wheel'>
      <inertial>
        <mass value="0.5" />
        <inertia ixx="0.0029" ixy="0" ixz="0" iyy="0.0029" iyz="0" izz="0.0056" />
      </inertial>
      <visual name='visual'>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <cylinder radius="0.15" length="0.05" />
        </geometry>
      </visual>
      <collision name='collision'>
        <origin xyz="0 0 0" rpy="-1.57 0 0"/>
        <geometry>
          <sphere radius="0.15" />
        </geometry>
      </collision>
    </link>

    <link name='right_wheel'>
      <inertial>
        <mass value="0.5" />
        <inertia ixx="0.0029" ixy="0" ixz="0" iyy="0.0029" iyz="0" izz="0.0056" />
      </inertial>
      <visual name='visual'>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <cylinder radius="0.15" length="0.05" />
        </geometry>
      </visual>
      <collision name='collision'>
        <origin xyz="0 0 0" rpy="-1.57 0 0"/>
        <geometry>
          <sphere radius="0.15" />
        </geometry>
      </collision>
    </link>


    <link name="base_scan">
      <visual>
        <origin xyz="0.0 0 0.15" rpy="0 0 0"/>
        <geometry>
          <cylinder length="0.0315" radius="0.055"/>
        </geometry>
        <material name="dark"/>
      </visual>

      <collision>
        <origin xyz="0.0 0 0.15" rpy="0 0 0"/>
        <geometry>
          <cylinder length="0.0315" radius="0.055"/>
        </geometry>
      </collision>

      <inertial>
        <mass value="0.114" />
        <origin xyz="0 0 0" />
        <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
      </inertial>
    </link>

    <joint name="scan_joint" type="fixed">
      <parent link="chassis"/>
      <child link="base_scan"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <joint name='left_wheel_joint' type='revolute'>
      <parent link="chassis" />
      <child link="left_wheel"/>
      <axis xyz="0 0 1" />
      <limit upper="1.79769e+308" lower="1.79769e+308" effort="1000" velocity="1000.00"/>

    </joint>

    <joint name='right_wheel_joint' type='revolute'>
      <parent link="chassis" />
      <child link="right_wheel"/>
      <axis xyz="0 0 1" />
      <limit upper="1.79769e+308" lower="1.79769e+308" effort="1000" velocity="1000.00"/>
    </joint>

    <joint name='caster_wheel' type="fixed">
      <parent link="chassis" />
      <child link="caster"/>
      <origin xyz="-0.3 0 -0.125" rpy="0 0 0"/>
    </joint>

    <joint name='caster_front_wheel' type="fixed">
      <parent link="chassis" />
      <child link="caster_front"/>
      <origin xyz="0.3 0 -0.125" rpy="0 0 0"/>
    </joint>

    



</robot>